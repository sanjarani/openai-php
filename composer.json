{
    "name": "sanjarani/openai-php",
    "description": "A PHP package for OpenAI API integration with Laravel support",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sanjarani\\OpenAI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sanjarani\\OpenAI\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sanjarani\\OpenAI\\OpenAIServiceProvider"
            ],
            "aliases": {
                "OpenAI": "Sanjarani\\OpenAI\\Facades\\OpenAI"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 