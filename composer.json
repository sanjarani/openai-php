{
    "name": "sanjarani/openai",
    "description": "A PHP package for easy integration with OpenAI API, including GPT-4, Assistants, and Tools support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiyarash Sanjarani Vahed",
            "email": "kiyarash.sanjarani@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sanjarani\\OpenAI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sanjarani\\OpenAI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sanjarani\\OpenAI\\OpenAIServiceProvider"
            ],
            "aliases": {
                "OpenAI": "Sanjarani\\OpenAI\\Facades\\OpenAI"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.1.6",
    "keywords": [
        "openai",
        "gpt",
        "gpt-4",
        "chatgpt",
        "ai",
        "laravel",
        "php",
        "assistants",
        "tools"
    ]
} 